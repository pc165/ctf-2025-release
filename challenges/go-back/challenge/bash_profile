#!/bin/bash

# Define colors
RED='\033[0;31m'
GREEN='\033[0;32m'
YELLOW='\033[0;33m'
BLUE='\033[0;34m'
MAGENTA='\033[0;35m'
CYAN='\033[0;36m'
WHITE='\033[0;37m'
RESET='\033[0m'

echo -e "${CYAN}./go-back${RESET} can read the secret flag.txt file!"
echo -e "Can you reveal its secrets?"
echo ''
echo -e "The source + Makefile are in ${CYAN}src/${RESET} if you want to mess around!"
echo -e "You can add your own files or do whatever you like in that folder"
echo -e
echo -e "But ultimately you need to use ${CYAN}./go-back${RESET} to read the flag"
